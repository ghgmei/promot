			private function faultHandler(event):void
			{
				
				
				//	After Unsuccessfull Submission - Tablet stores data until next login
				
				
				/**
				 * 
				 * Saves data to local database with check field set to 1
				 * ______________________________________________________
				 * 1 = saved to local with no server connection
				 * 0 = saved to local with server connection
				 * 2 = saved to local with manual sync 
				 * 
				 * */
				
				trace("Saving data to local database with check field set to 1");
				
				CHK = "1";
				
				var statement:SQLStatement = new SQLStatement();
				statement.sqlConnection = dbConnection;
				statement.text ="CREATE TABLE IF NOT EXISTS patient_data (id INTEGER PRIMARY KEY AUTOINCREMENT, dateAdded TEXT, hemoglobin TEXT, healthFacility TEXT, familyName TEXT, fname TEXT, nationality TEXT, nationalityOther TEXT, countryOfResidence TEXT, age TEXT, ageType TEXT, sex TEXT, mobile1 TEXT, mobile2 TEXT, pregnant TEXT, weight TEXT, q12 TEXT, q12 TEXT, q13 TEXT, q13Other TEXT, q14 TEXT, q15DailyDose TEXT, q15DailyDoseOther TEXT, q15Days TEXT, q15DaysOther TEXT, tabletId TEXT, interviewer TEXT, dateModified TEXT, patientid TEXT)";
				
				statement.execute();
				
				// Saves to DB
				var SQLStatement1:SQLStatement = new SQLStatement();
				SQLStatement1.sqlConnection = dbConnection;
				SQLStatement1.text ="INSERT INTO patient_data (dateAdded, hemoglobin, healthFacility, familyName, fname, nationality, nationalityOther, countryOfResidence, age, ageType, sex, mobile1, mobile2, pregnant, weight, q12, q12, q13, q13Other, q14, q15DailyDose, q15DailyDoseOther, q15Days, q15DaysOther, tabletId, interviewer, dateModified, patientid) VALUES ('"+date1+"','"+hemoglobin_measure+"','"+healthFacility+"','"+familyname.text+"','"+firstname.text+"','"+nationality+"','"+q6_specify_other.text+"','"+countryOfResidence+"','"+ageType+"','"+genders.text+"','"+mobile1.text+"','"+mobile2.text+"','"+pregnancy+"','"+weight.text+"','"+q12+"','"+q12+"','"+q13+"','"+q16_other_specify.text+"','"+q14+"','"+q18a+"','"+q18a_other+"','"+q18b+"','"+q18c+"','"+tabletID+"','"+interviewerName+"','"+date1+"','"+patientID+"')";
				
				SQLStatement1.execute();
				
				navigator.pushView(addedPatientView);
		
			}
			
